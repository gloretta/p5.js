function setup() {
  createCanvas(400, 400);
  background(114, 138, 224);
  angleMode(DEGREES);
  noStroke();
  
  //back main tower
  fill(194, 80, 21);
  triangle(50, 310, 200, -200, 350, 310);
  
  
  //tower decorations
  
  //middle row
  fill(173, 57, 3);
  arc(150, 75, 60, 50, 180, 0);
  arc(210, 75, 60, 50, 180, 0);
  arc(260, 75, 40, 50, 180, 0);
  fill(253, 202, 116);
  arc(150, 75, 50, 40, 180, 0);
  arc(210, 75, 50, 40, 180, 0);
  arc(260, 75, 30, 40, 180, 0);
  ellipse(180, 50, 10);
  ellipse(240, 50, 10);
  
  
  //bottom row
  fill(173, 57, 3);
  arc(150, 115, 60, 50, 180, 0);
  arc(210, 115, 60, 50, 180, 0);
  arc(260, 115, 60, 50, 180, 0);
  fill(253, 202, 116);
  arc(150, 115, 50, 40, 180, 0);
  arc(210, 115, 50, 40, 180, 0);
  arc(260, 115, 50, 40, 180, 0);
  ellipse(180, 90, 10);
  ellipse(240, 90, 10);
  
  //top row
  fill(173, 57, 3);
  arc(156, 35, 46, 50, 180, 0);
  arc(210, 35, 60, 50, 180, 0);
  arc(254, 35, 30, 44, 180, 0);
  fill(253, 202, 116);
  arc(155, 35, 36, 40, 180, 0);
  arc(210, 35, 50, 40, 180, 0);
  arc(254, 35, 20, 36, 180, 0);
  ellipse(180, 10, 10);
  ellipse(240, 10, 10);
  
  
  //Yellow and green bezier
  fill(210, 153, 48);
  beginShape();
  vertex(30, 130);
  bezierVertex(0, 80, 80, 80, 80, 30);
  bezierVertex(80, 80, 165, 80, 130, 130);
  endShape();
  noFill();
  stroke(2, 117, 12);
  strokeWeight(5);
  bezier(30, 130, 20, 70, 100, 100, 80, 40);
  bezier(60, 130, 60, 70, 120, 100, 83, 50);
  bezier(90, 130, 80, 90, 140, 120, 90, 60);
  bezier(120, 130, 100, 90, 160, 120, 90, 60);

  //tower for yellow and green bezier
  noStroke();
  strokeWeight(1);
  fill(207, 76, 43);
  rect(30, 130, 100, 20);
  rect(40, 150, 80, 180);
  fill(2, 117, 12);
  rect(40, 235, 80, 15);
  fill(240);
  arc(47, 250, 16, 30, 180, 0);
  arc(75, 250, 40, 30, 180, 0);
  arc(107.5, 250, 24, 30, 180, 0);
  fill(207, 76, 43);
  arc(47, 250, 12, 25, 180, 0);
  arc(75, 250, 34, 25, 180, 0);
  arc(107.5, 250, 20, 25, 180, 0);
  fill(240);
  rect(50, 170, 5, 40);
  rect(100, 170, 5, 40);
  rect(75, 170, 5, 40);
  rect(40, 150, 80, 5);
  
  
  fill(240);
  beginShape();
  vertex(90, 170);
  bezierVertex(55, 120, 140, 120, 140, 70);
  bezierVertex(140, 120, 225, 120, 190, 170);
  endShape();
  
  //stripes of blue and white bezier
  stroke(60, 55, 196);
  strokeWeight(4);
  noFill();
  line(140, 70, 140, 170);
  bezier(120, 170, 120, 120, 130, 150, 140, 70);
  bezier(90, 170, 80, 120, 110, 150, 140, 70);
  bezier(100, 170, 100, 120, 120, 150, 140, 70);
  bezier(160, 170, 160, 120, 150, 150, 140, 70);
  bezier(180, 170, 180, 120, 160, 150, 140, 70);
  bezier(190, 170, 200, 120, 170, 150, 140, 70);
  
  //tower for blue and white bezier
  noStroke();
  fill(60, 55, 196);
  rect(90, 170, 100, 5);
  fill(224, 98, 25);
  strokeWeight(2);
  stroke(17, 103, 69);
  rect(100, 175, 80, 150);
  noStroke();
  fill(240);
  rect(160, 190, 5, 40);
  rect(135, 190, 5, 40);
  rect(110, 190, 5, 40);
  
  fill(2, 117, 12);
  rect(100, 260, 80, 15);
  fill(240);
  arc(107, 275, 16, 30, 180, 0);
  arc(135, 275, 40, 30, 180, 0);
  arc(167.5, 275, 24, 30, 180, 0);
  fill(207, 76, 43);
  arc(107, 275, 12, 25, 180, 0);
  arc(135, 275, 34, 25, 180, 0);
  arc(167.5, 275, 20, 25, 180, 0);
  
  
  noStroke();
  strokeWeight(1);
  fill(237, 181, 50);
  beginShape();
  vertex(270, 210);
  bezierVertex(235, 170, 300, 170, 300, 140);
  bezierVertex(300, 170, 365, 170, 330, 210);
  endShape();  
  
  //yellow and green patterns
  stroke(2, 117, 12);
  strokeWeight(4);
  noFill();
  bezier(260, 190, 286, 167, 284, 164, 284, 164);
  bezier(265, 205, 290, 186, 305, 155, 305, 155);
  bezier(280, 210, 300, 190, 317, 170, 317, 165);
  bezier(300, 210, 309, 196, 329, 180, 329, 175);
  bezier(320, 210, 320, 207, 339, 190, 339, 185);
  
  line(339, 190, 297, 150);
  line(335, 204, 289, 160);
  line(320, 210, 275, 168);
  line(300, 210, 264, 178);
  line(280, 210, 260, 192);
  
  noStroke();
  fill(234, 173, 11);
  rect(270, 210, 60, 10);
  fill(224, 98, 25);
  strokeWeight(3);
  stroke(173, 69, 16);
  rect(270, 220, 60, 100);
  noStroke();
  
  fill(240);
  noStroke();
  rect(310, 230, 5, 20);
  rect(290, 230, 5, 20);
  
  //red and green bezier
  fill(148, 36, 13);
  beginShape();
  vertex(340, 150);
  bezierVertex(305, 100, 390, 100, 390, 50);
  bezierVertex(390, 100, 475, 100, 440, 150);
  endShape();

  
  //red and green bezier
  fill(234, 173, 11);
  rect(340, 150, 60, 10);
  fill(224, 98, 25);
  stroke(17, 103, 69);
  rect(340, 160, 60, 150);
  noStroke();
  
  //toppers
  fill(254, 192, 5);
  ellipse(80, 35, 7);
  ellipse(140, 65, 7);
  ellipse(300, 140, 7);
  ellipse(390, 55, 7);
  stroke(254, 192, 5);
  strokeWeight(2);
  line(80, 35, 80, 0);
  line(140, 65, 140, 35);
  line(300, 140, 300, 115);
  line(390, 55, 390, 20);
  
  fill(240);
  noStroke();
  rect(390, 180, 5, 40);
  rect(370, 180, 5, 40);
  rect(350, 180, 5, 40);
  
  //////////////////////////////////////Matryoshka//////////////////////////////////////////
  strokeWeight(1);
  noStroke();
  //body
  fill(255, 39, 39);
  ellipse(200, 230, 170, 200);
  
  fill(0);
  ellipse(200, 220, 150, 170);
  
  //head
  fill(247, 0, 33);
  ellipse(200, 120, 100,110);
  
  //rectangle that gives the body its shape
  fill(17, 103, 69);
  rect(0, 310, 400, 100);
  
  //stand
  fill(166, 18, 18);
  quad(150, 310, 250, 310, 260, 320, 140, 320);
  
  
  //head wear - 1 
