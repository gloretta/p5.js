  /*
  noFill();
  bezier(260, 150, 215, 110, 300, 110, 300, 80);
  bezier(300, 80, 300, 110, 385, 110, 340, 150);
  
  noFill();
  bezier(250, 150, 215, 100, 300, 100, 300, 50);
